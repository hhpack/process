{
  "name": "hhpack/process",
  "keywords": [
    "process", "utility", "simple", "easy", "hack"
  ],
  "description": "Process utility library for Hack",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "hhvm": ">=3.21.0",
    "hhvm/hhvm-autoload": "^1.5"
  },
  "require-dev": {
    "hackpack/hackunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "HHPack\\Process\\": "src/"
    },
    "files": [
      "src/bootstrap.hh",
      "src/shortcut.hh"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HHPack\\File\\Example\\": "example/custom/src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/holyshared/HackUnit"
    }
  ],
  "scripts": {
    "test": "vendor/bin/hackunit test"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}

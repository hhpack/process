{
  "name": "hhpack/process",
  "keywords": [
    "process", "utility", "simple", "easy", "hack"
  ],
  "description": "Process utility library for Hack",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "hhvm": ">=3.18.0"
  },
  "require-dev": {
    "hackpack/hackunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "HHPack\\Process\\": "src/"
    },
    "files": [
      "src/bootstrap.hh",
      "src/shortcut.hh"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HHPack\\File\\Example\\": "example/custom/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/hackunit test",
    "check": "./typecheck"
  },
  "minimum-stability": "stable"
}
